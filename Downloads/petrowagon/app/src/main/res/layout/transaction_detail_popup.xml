    <LinearLayout
        android:layout_marginTop="@dimen/space15"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">





            <TableLayout
              android:layout_marginLeft="@dimen/space15"
                android:layout_marginRight="@dimen/space15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Product Name :"
                        android:textColor="@color/black"
                        android:fontFamily="@font/humans"
                        android:textSize="@dimen/text18"
                        android:layout_column="1"

                        />

                    <TextView
                        android:layout_marginLeft="@dimen/space15"
                        android:id="@+id/proNameTransaction"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Product Name :"
                        android:textColor="@color/black"
                        android:fontFamily="@font/humans"
                        android:textSize="@dimen/text18"
                        android:layout_column="2"
                        />

                </TableRow>

                <TableRow
                    android:layout_marginTop="@dimen/space8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:layout_marginTop="@dimen/space8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Quantity :"
                        android:textColor="@color/black"
                        android:fontFamily="@font/humans"
                        android:textSize="@dimen/text18"
                        android:layout_column="1"
                        />

                    <TextView
                        android:layout_marginLeft="@dimen/space15"
                        android:id="@+id/proQuantityTransaction"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Quantity :"
                        android:textColor="@color/black"
                        android:fontFamily="@font/humans"
                        android:textSize="@dimen/text18"
                        android:layout_column="2"
                        />
                </TableRow>

                <TableRow
                    android:layout_marginTop="@dimen/space8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:layout_marginTop="@dimen/space8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Location :"
                        android:textColor="@color/black"
                        android:fontFamily="@font/humans"
                        android:textSize="@dimen/text18"
                        android:layout_column="1"
                        />


                    <TextView
                        android:ellipsize="marquee"
                        android:id="@+id/proLocationTransaction"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textMultiLine"
                        android:layout_column="2"
                        android:layout_marginLeft="@dimen/space15"
                        android:fontFamily="@font/humans"
                        android:text="Location :"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text18" />
                </TableRow>

                <TableRow
                    android:layout_marginTop="@dimen/space8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >
                    <TextView
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:layout_marginTop="@dimen/space8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Order Date :"
                        android:textColor="@color/black"
                        android:fontFamily="@font/humans"
                        android:textSize="@dimen/text18"
                        android:layout_column="1"
                        />

                    <TextView
                        android:layout_marginLeft="@dimen/space15"
                        android:id="@+id/proDateTransaction"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Order Date :"
                        android:textColor="@color/black"
                        android:fontFamily="@font/humans"
                        android:textSize="@dimen/text18"
                        android:layout_column="2"
                        />

                </TableRow>

                <TableRow
                    android:layout_marginTop="@dimen/space8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:layout_marginTop="@dimen/space8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Total Price:"
                        android:textColor="@color/black"
                        android:fontFamily="@font/humans"
                        android:textSize="@dimen/text18"
                        android:layout_column="1"
                        />

                    <TextView
                        android:layout_marginLeft="@dimen/space15"
                        android:id="@+id/proTotalPriceTransaction"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Total Price:"
                        android:textColor="@color/black"
                        android:fontFamily="@font/humans"
                        android:textSize="@dimen/text18"
                        android:layout_column="2"
                        />



                </TableRow>

            </TableLayout>




    <Button
        android:layout_marginBottom="@dimen/space20"
        android:layout_marginLeft="@dimen/space20"
        android:layout_marginRight="@dimen/space20"
        android:layout_marginTop="@dimen/space10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/humans"
        android:textColor="@color/black"
        android:background="@drawable/grey_corners"
        android:textSize="@dimen/text16"
        android:text="Back"
        android:id="@+id/BtnBackTransaction"/>

    </LinearLayout>



</androidx.cardview.widget.CardView>